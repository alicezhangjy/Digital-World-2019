#Question5
class Coordinate():
    x = 3.2
    y = -1.5  #these are just to initialise, is mutable

p1 = Coordinate ()
p2 = Coordinate ()
p2.x = 0.3
p2.y = 1.0
print(type(p1))
print(type(p2))
print(type(Coordinate))
print(p1.x, p1.y)
print(p2.x, p2.y)

